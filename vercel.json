{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": [
                "GET",
                "PUT",
                "PATCH",
                "POST",
                "DELETE",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "https://cashier-pos.netlify.app, http://localhost:5173",
                "Access-Control-Allow-Credentials": "true"
            }
        }
    ]
}